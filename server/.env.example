# Server Configuration
PORT=8800
NODE_ENV=development

# Database Configuration
DATABASE_URL="postgresql://db_owner:password@host/dbname"
DATABASE_URL_UNPOOLED="postgresql://db_owner:password@host/dbname"

# Security & Authentication
JWT_SECRET="your_jwt_secret_key_here"
ALLOWED_ORIGINS="http://localhost:5173,http://localhost:8800"

# Email Configuration (IMAP for bid emails)
IMAP_USER="your_email@gmail.com"
IMAP_PASSWORD="app_specific_password_or_real_password"
IMAP_HOST="imap.gmail.com"
IMAP_PORT=993
IMAP_TLS=true
BID_EMAIL_FOLDER="INBOX"